{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport API from './axiosInstance';\nexport default {\n  name: 'Home',\n  setup() {\n    const activeMenu = ref('home');\n    const latestBugs = ref([]);\n    const router = useRouter();\n    const fetchLatestBugs = async () => {\n      try {\n        const response = await API.get('/bugs/latest');\n        latestBugs.value = response.data;\n      } catch (error) {\n        console.error('Error fetching latest bugs:', error);\n      }\n    };\n    const handleMenuSelect = key => {\n      activeMenu.value = key;\n    };\n    const refreshPage = () => {\n      router.go(0);\n    };\n    onMounted(() => {\n      fetchLatestBugs();\n    });\n    return {\n      activeMenu,\n      latestBugs,\n      handleMenuSelect,\n      refreshPage\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","useRouter","API","name","setup","activeMenu","latestBugs","router","fetchLatestBugs","response","get","value","data","error","console","handleMenuSelect","key","refreshPage","go"],"sources":["D:\\Files\\Code\\CoursesProjects\\YacBugzilla_KnowledgeMap\\front-end\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\r\n  <div class=\"home\">\r\n    <!-- 导航栏 -->\r\n    <el-header>\r\n      <div class=\"logo\">Yocto项目分析平台</div>\r\n      <el-menu\r\n        class=\"el-menu-demo\"\r\n        v-model=\"activeMenu\"\r\n        :default-active=\"activeMenu\"\r\n        background-color=\"#545c64\"\r\n        text-color=\"#fff\"\r\n        active-text-color=\"#ffd04b\"\r\n        mode=\"horizontal\"\r\n        @select=\"handleMenuSelect\"\r\n      >\r\n        <el-menu-item index=\"home\">\r\n          <router-link to=\"/\" @click.native=\"refreshPage\">首页</router-link>\r\n        </el-menu-item>\r\n        <el-menu-item index=\"dataExhibition\">\r\n          <router-link to=\"/dataExhibition\">数据展览</router-link>\r\n        </el-menu-item>\r\n        <el-menu-item index=\"dataAnalysis\">\r\n          <router-link to=\"/dataAnalysis\">数据分析</router-link>\r\n        </el-menu-item>\r\n        <el-menu-item index=\"dataStatistics\">\r\n          <router-link to=\"/dataStatistics\">数据统计</router-link>\r\n        </el-menu-item>\r\n        <el-menu-item index=\"knowledgeMap\">\r\n          <router-link to=\"/knowledgeMap\">数据图谱</router-link>\r\n        </el-menu-item>\r\n        <el-menu-item index=\"predictionAnalysis\">\r\n          <router-link to=\"/predictionAnalysis\">预测分析</router-link>\r\n        </el-menu-item>\r\n      </el-menu>\r\n    </el-header>\r\n\r\n    <!-- 中部占位图片 -->\r\n    <el-main>\r\n      <img src=\"your-placeholder-image.jpg\" alt=\"占位图片\" class=\"center-image\" />\r\n    </el-main>\r\n\r\n    <!-- 资源分类列表图标 -->\r\n    <el-main class=\"resources\">\r\n      <div class=\"left\">\r\n        <h2>资源分类</h2>\r\n        <ul>\r\n          <li><i class=\"icon-overview\"></i> 总览</li>\r\n          <li><i class=\"icon-product-component\"></i> 产品组件</li>\r\n          <li><i class=\"icon-status\"></i> 状态</li>\r\n          <li><i class=\"icon-platform\"></i> 平台</li>\r\n          <li><i class=\"icon-company\"></i> 公司</li>\r\n        </ul>\r\n      </div>\r\n      <div class=\"right\">\r\n        <h2>最新10个bug</h2>\r\n        <ul>\r\n          <li v-for=\"bug in latestBugs\" :key=\"bug.id\">{{ bug.summary }}</li>\r\n        </ul>\r\n      </div>\r\n    </el-main>\r\n\r\n    <!-- 页脚 -->\r\n    <el-footer class=\"footer\">\r\n      <div class=\"footer-content\">\r\n        <p>版权所有 &copy; 2024 Yocto项目分析平台</p>\r\n      </div>\r\n    </el-footer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted } from 'vue';\r\nimport { useRouter } from 'vue-router';\r\nimport API from './axiosInstance';\r\n\r\nexport default {\r\n  name: 'Home',\r\n  setup() {\r\n    const activeMenu = ref('home');\r\n    const latestBugs = ref([]);\r\n    const router = useRouter();\r\n\r\n    const fetchLatestBugs = async () => {\r\n      try {\r\n        const response = await API.get('/bugs/latest');\r\n        latestBugs.value = response.data;\r\n      } catch (error) {\r\n        console.error('Error fetching latest bugs:', error);\r\n      }\r\n    };\r\n\r\n    const handleMenuSelect = (key) => {\r\n      activeMenu.value = key;\r\n    };\r\n\r\n    const refreshPage = () => {\r\n      router.go(0);\r\n    };\r\n\r\n    onMounted(() => {\r\n      fetchLatestBugs();\r\n    });\r\n\r\n    return {\r\n      activeMenu,\r\n      latestBugs,\r\n      handleMenuSelect,\r\n      refreshPage,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.logo {\r\n  font-size: 1.5em;\r\n  font-weight: bold;\r\n  color: #fff;\r\n}\r\n.center-image {\r\n  display: block;\r\n  margin: 0 auto;\r\n  max-width: 100%;\r\n}\r\n.resources {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  padding: 20px;\r\n}\r\n.left, .right {\r\n  width: 48%;\r\n}\r\n.left ul, .right ul {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\n.left ul li, .right ul li {\r\n  margin: 10px 0;\r\n}\r\n.left i, .right i {\r\n  margin-right: 10px;\r\n}\r\n.footer {\r\n  text-align: center;\r\n  padding: 20px;\r\n  background-color: #545c64;\r\n  color: #fff;\r\n}\r\n</style>\r\n"],"mappings":"AAuEA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,SAAQ,QAAS,YAAY;AACtC,OAAOC,GAAE,MAAO,iBAAiB;AAEjC,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,KAAKA,CAAA,EAAG;IACN,MAAMC,UAAS,GAAIN,GAAG,CAAC,MAAM,CAAC;IAC9B,MAAMO,UAAS,GAAIP,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMQ,MAAK,GAAIN,SAAS,CAAC,CAAC;IAE1B,MAAMO,eAAc,GAAI,MAAAA,CAAA,KAAY;MAClC,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,GAAG,CAACQ,GAAG,CAAC,cAAc,CAAC;QAC9CJ,UAAU,CAACK,KAAI,GAAIF,QAAQ,CAACG,IAAI;MAClC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,6BAA6B,EAAEA,KAAK,CAAC;MACrD;IACF,CAAC;IAED,MAAME,gBAAe,GAAKC,GAAG,IAAK;MAChCX,UAAU,CAACM,KAAI,GAAIK,GAAG;IACxB,CAAC;IAED,MAAMC,WAAU,GAAIA,CAAA,KAAM;MACxBV,MAAM,CAACW,EAAE,CAAC,CAAC,CAAC;IACd,CAAC;IAEDlB,SAAS,CAAC,MAAM;MACdQ,eAAe,CAAC,CAAC;IACnB,CAAC,CAAC;IAEF,OAAO;MACLH,UAAU;MACVC,UAAU;MA